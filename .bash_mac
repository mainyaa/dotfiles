# Boot SSH Agent
#eval `ssh-agent`;

export MVIM_HOME=/Applications/MacVim.app/Contents/MacOS
export_path_if_exists $MVIM_HOME
export EDITOR=$MVIM_HOME/MacVim
alias vi='LANG=ja_JP.UTF-8 $MVIM_HOME/Vim $@'
alias vim='LANG=ja_JP.UTF-8 $MVIM_HOME/Vim $@'
alias mvim='LANG=ja_JP.UTF-8 $MVIM_HOME/MacVim $@'
#alias vim='LANG=ja_JP.UTF-8 $MVIM_HOME/MacVim $@'
alias mldev='ssh -A mldev mldev'

export HDF5_DIR=/opt/local
export CLOUD_SDK_HOME=$HOME/google-cloud-sdk
#source /usr/local/share/zsh/site-functions/_aws
#export AWS_RDS_HOME=$HOME/local/RDSCli-1.15.001
#export AWS_CREDENTIAL_FILE=$AWS_RDS_HOME/.credential
export GNUBIN_HOME=HOMEBREW_PREFIX/opt/coreutils/libexec/gnubin

export PATH=$GNUBIN_HOME:$PATH:$MVIM_HOME:$CLOUD_SDK_HOME/bin:$AWS_RDS_HOME/bin:$ANDROID_HOME/bin

